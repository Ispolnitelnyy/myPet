{
   "name": "mypet",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "start": "webpack server --env port=3000",
      "build:dev": "webpack  --env mode=development",
      "build:prod": "webpack  --env mode=production",
      "decloration": "npx typed-scss-modules src/",
      "lint:ts": "eslint \"**/*.{ts,tsx}\"",
      "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
      "stylelint:scss": "npx stylelint \"**/*.scss\"",
      "stylelint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
      "test:unit": "jest --config ./config/tests/jest.config.ts",
      "test:ui": "npx loki init --config ./config/storybook",
      "test:ui:ok": "npx loki approve",
      "test:ui:ci": "npm run update-loki-timeout && npx loki --requireReference --reactUri file:./storybook-static",
      "storybook": "storybook dev -p 6006 -c ./config/storybook",
      "storybook:build": "storybook build -c ./config/storybook",
      "update-loki-timeout": "ts-node scripts/updateLokiTimeout.ts",
      "loki-test": "npm run update-loki-timeout && npx loki test"
   },
